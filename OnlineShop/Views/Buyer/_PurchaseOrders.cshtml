@using OnlineShop.Models;
@model List<OrderModel> 

<table width="100%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Category</th>
            <th>Price</th>
            <th>Amount</th>
            <th>Total price</th>
        </tr>
    </thead>

    <tbody>
        @if (Model != null && Model.Count > 0)
        {
            for (var i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td>@Model[i].Name</td>
                    <td>@Model[i].Category</td>
                    <td>@Model[i].Price</td>
                    <td>@Model[i].Amount</td>
                    <td>@Model[i].TotalPrice</td>                    
                </tr>
            }
        }
    </tbody>
</table>